{
  "name": "pulsar-ide-rust",
  "main": "./lib/index",
  "version": "1.1.2",
  "description": "Rust language support for Pulsar Edit",
  "repository": "https://github.com/Skeletonxf/pulsar-ide-rust/",
  "license": "MIT",
  "keywords": [
    "rust",
    "rls",
    "ide"
  ],
  "package-deps": [
    "atom-ide-base"
  ],
  "engines": {
    "atom": ">=1.33.0"
  },
  "dependencies": {
    "atom-languageclient": "^1.14.1",
    "atom-package-deps": "^8.0.0",
    "toml": "^3.0.0",
    "underscore-plus": "^1.7.0"
  },
  "devDependencies": {
    "atom-jasmine3-test-runner": "^5.2.7",
    "eslint-config-atomic": "^1.16.2",
    "prettier-config-atomic": "^2.0.5"
  },
  "scripts": {
    "format": "prettier --write .",
    "test.format": "prettier . --check",
    "lint": "eslint . --fix",
    "test.lint": "eslint .",
    "test": "atom --test spec"
  },
  "prettier": "prettier-config-atomic",
  "atomTestRunner": "./spec/runner",
  "enhancedScopes": [
    "source.rust"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
